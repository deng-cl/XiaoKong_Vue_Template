{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "types/*.d.ts", "src/**/*.md"],
    "exclude": ["src/**/__tests__/*", "dist", "**/*.js", "node_modules"],
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "paths": {
            "@/*": ["./src/*"]
        },
        "noImplicitAny": false
    }
}
